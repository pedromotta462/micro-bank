# API Gateway Configuration
NODE_ENV=production
PORT=3000

# Application
APP_NAME=api-gateway
APP_VERSION=1.0.0

# Logging
LOG_LEVEL=info

# CORS
CORS_ENABLED=true
CORS_ORIGINS=*

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100

# JWT Authentication
JWT_SECRET=your-secret-key-change-in-production-minimum-32-characters
JWT_EXPIRES_IN=24h
